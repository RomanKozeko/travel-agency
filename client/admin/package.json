    "draft-js": "^0.10.3",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint-config-airbnb": "^16.0.0",
    "lodash.uniqueid": "^4.0.1",
    "material-ui": "^1.0.0-beta.16",
    "material-ui-icons": "^1.0.0-beta.15",
    "normalizr": "^3.2.4",
